# Server Configuration
NODE_ENV=development
PORT=3000
API_URL=http://localhost:3000

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/habit_tracker

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
